{
	"name": "platform-grote-themas",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate-graphql": "graphql-codegen -r dotenv/config",
		"watch-graphql": "graphql-codegen --watch -r dotenv/config"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.0.0",
		"@graphql-codegen/introspection": "^3.0.0",
		"@graphql-codegen/schema-ast": "^3.0.0",
		"@graphql-codegen/typescript": "^3.0.0",
		"@graphql-codegen/typescript-graphql-request": "^4.5.8",
		"@graphql-codegen/typescript-operations": "^3.0.0",
		"@poppanator/sveltekit-svg": "^3.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql-codegen-plugin-typescript-swr": "^0.8.3",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.0.3",
		"graphql": "^16.6.0",
		"graphql-request": "^5.1.0"
	}
}
